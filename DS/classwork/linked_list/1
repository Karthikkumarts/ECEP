#include<stdio.h>
#include<stdlib.h>
typedef struct node
{
    int data;
    struct node * link;
}slist;

slist *top = NULL;

void push(int i)
{
    printf("push---\n");
    slist *new;
    new = (slist *)malloc(sizeof(slist));

    printf("push--- 2\n");
    new->data = i;
    new->link = top;
    top = new;
}
void pop()
{
slist *temp=top;
if(temp == NULL)
{
    printf("Stack underflow\n");
}
top = temp->link;
free(temp);
}
void display()
{
       printf("display\n");
   while(top->link != NULL)
   {
       top=top->link;
   }
}
int main()
{
    push(1);
    push(2);
    push(3);
    display();
    pop();
    display();
}

